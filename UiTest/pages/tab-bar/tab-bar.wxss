.demo {
  font-family: Arial, Helvetica, sans-serif;
}
.demo .mb-12 {
  margin-bottom: 24rpx;
  display: block;
}
.demo .mg-16 {
  margin: 32rpx;
}
.demo .t-tab-bar {
  position: relative;
  padding-bottom: 0;
}
.demo .t-badge--basic:not(.t-badge--dot) {
  padding: 0 10rpx;
}
.demo .demo-word {
  margin-top: 364rpx;
}
.demo .bottom-row {
  padding-bottom: env(safe-area-inset-bottom);
}
.demo .t-tab-bar-item__spread-item {
  color: rgba(0, 0, 0, 0.6);
}
.demo .t-tab-bar-item__content:not(.t-is-checked) .t-tab-bar-item__text {
  color: rgba(0, 0, 0, 0.6) !important;
}
