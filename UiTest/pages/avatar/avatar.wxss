page {
  background: #fff;
}
.desc {
  margin: 10rpx 32rpx;
  color: #999;
  font-size: 26rpx;
  line-height: 36rpx;
}
.row {
  display: flex;
  width: 100%;
  margin: 20rpx 0;
}
.row .avatar-example {
  text-align: center;
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
}
.row .avatar-example .avatar-word {
  margin-top: 16rpx;
}
.row .text {
  width: 130rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.row .row-text-64 {
  width: 128rpx;
}
.row .row-text-48 {
  width: 96rpx;
}
.row .row-text-40 {
  width: 80rpx;
}
.row .row-text-32 {
  width: 64rpx;
}
.row .row-text-24 {
  width: 48rpx;
}
.row .row-text-common {
  text-align: center;
}
.row .font-size-20 {
  font-size: 40rpx;
}
.row .border-size-2 {
  border: 4rpx solid #fff;
}
.row .border-size-3 {
  border: 6rpx solid #fff;
}
.row .border-size-4 {
  border: 8rpx solid #fff;
}
.row .excursion-right-8 {
  margin-right: -16rpx;
}
.row .alt-example {
  color: #fff;
  background-color: #0052d9;
  font-weight: 400;
}
.bottom-row {
  padding-bottom: 96rpx;
}
.gutter-column-12 {
  width: 24rpx;
  display: inline-block;
}
.gutter-column-24 {
  width: 48rpx;
  display: inline-block;
}
.gutter-column-32 {
  width: 64rpx;
  display: inline-block;
}
.gutter-column-64 {
  width: 128rpx;
  display: inline-block;
}
